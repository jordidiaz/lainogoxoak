{
  "name": "lainogoxoak",
  "version": "0.1.0",
  "homepage": "https://bitbucket.org/jordi_diaz/lainogoxoak",
  "authors": [
    "Jordi Díaz Añorga <diaz.jordi@gmail.com>"
  ],
  "description": "Brunch skeleton for static websites",
  "keywords": [
    "brunch",
    "static"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "bootstrap": "3.2.0",
    "font-awesome": "4.2.0",
    "respond": "1.4.2",
    "jquery": "1.9.1",
    "modernizr": "2.8.3",
    "jquery-waypoints": "2.0.3",
    "jquery-backstretch": "2.0.3",
    "i18next": "~1.8.0"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "less/bootstrap.less",
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js"
      ]
    },
    "font-awesome": {
      "main": "css/font-awesome.css"
    },
    "flexslider": {
      "main": [
        "flexslider.less",
        "flexslider.css",
        "jquery.flexslider.js"
      ]
    },
    "modernizr": {
      "main": "modernizr.js"
    },
    "jquery": {
      "main": "jquery.js"
    },
    "jquery-masonry": {
      "main": "jquery.masonry.js"
    },
    "jquery-waypoints": {
      "main": "waypoints.js"
    },
    "jquery-backstretch": {
      "main": "jquery.backstretch.js"
    }
  }
}
